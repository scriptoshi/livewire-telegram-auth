{
    "name": "scriptoshi/livewire-telegram-auth",
    "description": "Laravel Livewire component for Telegram login authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Scriptoshi",
            "email": "author@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.0",
        "tg/tgwebvalid": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Scriptoshi\\LivewireTelegramAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scriptoshi\\LivewireTelegramAuth\\Providers\\LivewireTelegramAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}